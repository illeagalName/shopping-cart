<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lhy.shopping.cart.dao.AccountDAO">
    <select id="findAccount" resultType="com.lhy.shopping.cart.entity.AccountDO">
        select *
        from account
        where user_name = #{userName}
    </select>
</mapper>